{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "arkissearch",
    "namespace": "arkis",
    "creationTimestamp": null
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "creationTimestamp": null,
        "labels": {
          "service": "arkissearch"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "arkissearch",
            "image": "chumbo/arkissearch:3.0",
            "ports": [
              {
                "containerPort": 55555,
                "protocol": "TCP"
              }
            ],
            "env": [
              {
                "name": "DATABASE_HOST",
                "value": "arkismongopersistent"
              },
              {
                "name": "DATABASE_PORT",
                "value": "30010"
              }
            ],
            "resources": {}
          }
        ],
        "restartPolicy": "Always"
      }
    },
    "strategy": {}
  },
  "status": {}
}