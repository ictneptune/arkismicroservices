{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "arkismysql",
    "namespace": "arkis",
    "creationTimestamp": null
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "creationTimestamp": null,
        "labels": {
          "service": "arkismysql"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "mysql",
            "image": "mysql:5.6",
            "env": [{
              "name": "MYSQL_DATABASE",
              "value": "database"},{
              "name": "MYSQL_ROOT_PASSWORD",
              "value": "root"},{
              "name": "MYSQL_USER",
              "value": "ramz"},{
              "name": "MYSQL_PASSWORD",
              "value": "ramz"}],
            "ports": [
              {
                "containerPort": 3306,
                "protocol": "TCP"
              }
            ],
            "volumeMounts": [
              {
                "name": "mysql-persistent-storage",
                "mountPath": "/var/lib/mysql"
              }
          ],
            "resources": {}
          }
        ],
        "volumes": [
          {
            "name": "mysql-persistent-storage",
            "gcePersistentDisk": {
              "pdName": "disk-mysql-0",
              "fsType": "ext4"
            }
          }
        ],
        "restartPolicy": "Always"
      }
    },
    "strategy": {}
  },
  "status": {}
}